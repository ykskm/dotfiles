[alias]
    ci = commit
    cia = commit --amend -C HEAD
    co = checkout
    br = branch
    st = status
    lg = log --name-status
    d  = diff
    dc = diff --cached

[color]
    diff = auto
    branch = auto
    status = auto
    interactive = auto

[core]
    excludesfile = "~/.gitignore_global"

[i18n]
    commitencoding = utf-8

[push]
    default = tracking

